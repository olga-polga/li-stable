{
  "name": "Listable-demo",
  "version": "1.0.0",
  "description": "",
  "dependencies": {
    "all": "^0.0.0",
    "aphrodite": "^1.2.5",
    "axios": "^0.17.1",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.5",
    "react-fa": "^5.0.0",
    "react-fa-icon": "^0.1.4",
    "react-images": "^0.5.13",
    "react-scripts": "^1.0.17",
    "react-toastr": "^3.0.0",
    "serve": "^6.4.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start": "react-scripts start",
    "dev": "npm-run-all -p watch-css start",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "deploy": "yarn run build && cf push -p public",
    "gcp": "yarn run build && gcloud app deploy --promote"
  },
  "keywords": [],
  "author": "Olga <olga.penina@dell.com>",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.1.2",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-adapter-react-helper": "^1.1.0",
    "jsdom": "^9.3.0",
    "mocha": "^4.0.1",
    "react-test-renderer": "^15.6.2",
    "sinon": "^4.1.2"
  },
  "engines": {
    "node": "9.x"
  },
  "repository": "https://github.com/olga-polga/listable.git"
}
